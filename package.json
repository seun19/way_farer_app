{
  "name": "way-farer-cycle-47",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./build/bin/www",
    "debug": "set DEBUG=Way_Farer_Cycle_47:* & npm start",
    "devStart": "set DEBUG=dev,Way_Farer_Cycle_47:* & nodemon --exec babel-node ./src/bin/www",
    "build": "babel src --out-dir build --ignore src/mockapi,src/**/*.test.js",
    "lint": "eslint ./src  --fix",
    "test": "nyc mocha --require @babel/register src/**/*.test.js --exit",
    "test:coverage": "nyc --reporter=html --reporter=text mocha --require @babel/register src/**/*.test.js --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:coveralls": "npm run test && npm run coverage"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "pg": "^7.11.0",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.2",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.5.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
